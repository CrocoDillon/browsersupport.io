{
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "impliedStrict": true,
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "globals": {
    "Promise": true
  },
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "no-undef": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 2,
    "quotes": [2, "single"],
    "semi": [2, "never"]
  }
}
